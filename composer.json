{
    "name": "syntech/dashboard",
    "description": "A Laravel package to create custom controllers with associated resources",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Mokhtar Ali",
        "email": "cto@syntecheg.com"
      }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "yajra/laravel-datatables-buttons": "9.0",
        "yajra/laravel-datatables-oracle": "^10.3.1"
    },
    "autoload": {
      "psr-4": {
        "Syntech\\Dashboard\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Syntech\\Dashboard\\DashboardServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.2"
  }
